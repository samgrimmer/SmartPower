@page "/invoice/{spInvoiceNumber:int}"

@using SmartPower.Common

<BulkReversalBreadcrumbs></BulkReversalBreadcrumbs>

@if (InvoiceSummary == null)
{
    <Toaster ErrorMessage="true" Message="@Constant.Invoice.Message.NotFound"></Toaster>
}
else
{
    <table class="table">
        <InvoiceRowHeader></InvoiceRowHeader>
        <tbody>
            <InvoiceRow InvoiceSummary="@InvoiceSummary"></InvoiceRow>
        </tbody>
    </table>
}
